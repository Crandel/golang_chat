{
    "Version": "0.0.1",
    "Database": {
        "Type": "postgres",
        "Path": "host=postgres user=gouser dbname=godb sslmode=disable password=pass"
    },
    "Server": {
        "Host": "127.0.0.1",
        "Domain": "localhost",
        "Port": 8000,
        "UseHTTPS": false,
        "HTTPS": {
            "Port": 443,
            "Certificate": "/path/to/certificate",
            "Key": "/path/to/key"
        }
    },
    "Template": {
        "Root": "base",
        "Ext": "gtpl",
        "Folder": "templates"
    },
    "Session": {
        "SecretKey": "SecretKey",
        "Options": {
            "Path": "",
            "Domain": "localhost",
            "MaxAge": 28800,
            "Secure": false,
            "HttpOnly": false
        }
    }
}
